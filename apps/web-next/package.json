{
  "name": "web-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=./node_modules/full-icu next dev -p 3003",
    "ws:dev": "tsx scripts/dev-ws.ts",
    "dev:auto": "node scripts/dev-auto.mjs",
    "dev:unix": "sh ./scripts/predev.sh && rm -rf .next && WATCHPACK_POLLING=true WATCHPACK_POLLING_INTERVAL=2000 NEXT_WEBPACK_USEPERSISTENTCACHE=false next dev -p 3003 -H 0.0.0.0",
    "dev:win": "node scripts/dev-win.mjs",
    "build": "cross-env NODE_ICU_DATA=./node_modules/full-icu NEXT_DISABLE_ESLINT=1 next build",
    "postbuild": "node ../../tools/copy-standalone-assets.cjs",
    "start": "cross-env NODE_ICU_DATA=./node_modules/full-icu next start -p 3003",
    "start:prod:standalone": "node .next/standalone/server.js",
    "clean": "node -e \"const fs=require('fs'); for(const p of ['.next','.turbo']){try{fs.rmSync(p,{recursive:true,force:true});}catch(e){}} console.log('clean ok');\"",
    "dev:clean": "pnpm run clean && next dev --hostname localhost --port 3003",
    "build:direct": "cross-env NEXT_DISABLE_ESLINT=1 next build",
    "build:evidence": "node scripts/build-evidence.cjs",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:advisory": "eslint . --ext .ts,.tsx || true",
    "lint:next": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --passWithNoTests",
    "test:dashboard": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --testPathPatterns='dashboard' --passWithNoTests",
    "test:e2e": "playwright test --reporter=list",
    "capture:dashboard-v2-step3-2": "node scripts/capture-dashboard-v2-step3-2.cjs",
    "capture:dashboard-v2-step3-3": "node scripts/capture-dashboard-v2-step3-3.cjs",
    "capture:dashboard-v2-step3-4": "node scripts/capture-dashboard-v2-step3-4.cjs",
    "capture:dashboard-v2-step3-5": "node scripts/capture-dashboard-v2-step3-5.cjs",
    "capture:dashboard-overview-skeleton": "node scripts/capture-dashboard-overview-skeleton.cjs",
    "capture:dashboard-overview-contract-v1": "node scripts/capture-dashboard-overview-contract-v1.cjs",
    "capture:terminal-step-t1": "node scripts/capture-terminal-step-t1.cjs",
    "capture:terminal-step-t2": "node scripts/capture-terminal-step-t2.cjs",
    "capture:northstar-dashboard": "node scripts/capture-northstar-dashboard.cjs",
    "capture:northstar-terminal": "node scripts/capture-northstar-terminal.cjs",
    "capture:northstar-portfolio": "node scripts/capture-northstar-portfolio.cjs",
    "capture:dashboard-home-contract-v1-1": "node scripts/capture-dashboard-home-contract-v1-1.cjs",
    "capture:dashboard-console": "node scripts/capture-dashboard-console.cjs",
    "test:rightdock-layout": "node scripts/test-rightdock-layout.cjs",
    "i18n:check": "node ../../scripts/i18n-check.mjs",
    "check:bundles": "node scripts/check-next-bundles.mjs",
    "check:charts-api": "node ../../scripts/check-charts-api-drift.mjs",
    "smoke:system-status": "node scripts/smoke-system-status-and-dedupe.cjs",
    "collect:ui-root-cause": "node scripts/collect-ui-root-cause.cjs"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@types/ws": "^8.18.1",
    "lightweight-charts": "^5.0.9",
    "next": "14.2.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.2.1",
    "swr": "^2.3.6",
    "ws": "^8.18.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.5",
    "eslint-plugin-regex": "^1.10.0",
    "jest": "^30.2.0",
    "jest-config": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.18",
    "full-icu": "^1.5.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
