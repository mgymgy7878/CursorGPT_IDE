{
  "title": "Spark â€¢ Backtest Jobs",
  "tags": ["spark", "backtest"],
  "timezone": "browser",
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Jobs (1m)",
      "targets": [
        {
          "expr": "rate(spark_backtest_jobs_total[1m])"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Latency P95 (ms)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(spark_backtest_latency_ms_bucket[5m])))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Errors (5m)",
      "targets": [
        {
          "expr": "rate(spark_backtest_errors_total[5m])"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "schemaVersion": 38
}

