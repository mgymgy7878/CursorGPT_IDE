{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "composite": true
  },
  "include": [
    "src"
  ]
}
