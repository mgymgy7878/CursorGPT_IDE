{
  "name": "@spark/common",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "default": "./dist/*"
    }
  },
  "typesVersions": { "*": { "*": ["dist/*", "dist/index.d.ts"] } },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/qs": "^6.9.15",
    "express": "^4.19.2"
  }
} 