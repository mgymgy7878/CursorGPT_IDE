{
  "name": "@spark/exchange-btcturk",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./*": "./dist/*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsup src/index.ts --dts --format esm,cjs",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch"
  },
  "dependencies": {
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^18.19.122",
    "@types/ws": "^8.5.10",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": ">=5.4"
  }
}