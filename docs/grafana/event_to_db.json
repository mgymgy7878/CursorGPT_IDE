{
  "dashboard": {
    "id": null,
    "title": "Event to DB Latency",
    "tags": ["streams", "latency", "database"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Event to DB Latency P95",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(streams_event_to_db_ms_bucket[5m]))",
            "legendFormat": "P95 Event to DB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 300},
                {"color": "red", "value": 1000}
              ]
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 2,
        "title": "Event to DB Latency Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(streams_event_to_db_ms_bucket[1m])",
            "legendFormat": "{{le}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Event to DB Latency by Source",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(streams_event_to_db_ms_bucket[5m])) by (src)",
            "legendFormat": "{{src}} - P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(streams_event_to_db_ms_bucket[5m])) by (src)",
            "legendFormat": "{{src}} - P50"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0
          }
        ]
      },
      {
        "id": 4,
        "title": "Event to DB Latency Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(streams_event_to_db_ms_bucket[5m]))",
            "legendFormat": "P99"
          },
          {
            "expr": "histogram_quantile(0.95, rate(streams_event_to_db_ms_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.90, rate(streams_event_to_db_ms_bucket[5m]))",
            "legendFormat": "P90"
          },
          {
            "expr": "histogram_quantile(0.50, rate(streams_event_to_db_ms_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
} 