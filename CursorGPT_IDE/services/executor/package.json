{
  "name": "@spark/executor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "NODE_OPTIONS=--enable-source-maps ts-node-esm src/server.ts",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "prisma:generate:executor": "prisma generate --schema ./services/executor/prisma/schema.prisma",
    "db:push:executor": "prisma db push --schema ./services/executor/prisma/schema.prisma"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "8.4.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/rate-limit": "8.1.1",
    "@fastify/under-pressure": "^9.0.3",
    "@fastify/websocket": "8.3.1",
    "@prisma/client": "^5.22.0",
    "@spark/auth": "workspace:*",
    "@spark/exchange-binance": "workspace:*",
    "@spark/guardrails": "workspace:*",
    "@spark/security": "workspace:*",
    "@spark/shared": "workspace:*",
    "@spark/types": "workspace:*",
    "adm-zip": "^0.5.12",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "fastify": "4.29.1",
    "pino": "^9.12.0",
    "prom-client": "15.1.3",
    "rate-limiter-flexible": "^7.3.1",
    "undici": "^7.16.0",
    "ws": "^8.18.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.13",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "prisma": "^5.22.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "tsx": "^4.16.0",
    "typescript": "^5.9.2"
  }
}
