{
  "name": "@spark/analytics",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsup src/index.ts --dts --format esm,cjs",
    "clean": "rimraf dist",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "parity-report": "node dist/parity-report.js"
  },
  "dependencies": {
    "csv-writer": "^1.6.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.19.122",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  }
}
