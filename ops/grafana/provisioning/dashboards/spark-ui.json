{
  "dashboard": {
    "title": "Spark Platform - UI SLO",
    "tags": ["spark", "slo", "ui"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "P95 Latency",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "ui_latency_p95_ms",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 150, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "ui_error_rate_pct",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error %"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 5, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 3,
        "title": "Staleness",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "ui_staleness_sec",
            "legendFormat": "Staleness",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 30, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 4,
        "title": "Uptime",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "ui_uptime_min",
            "legendFormat": "Uptime",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "unit": "m"
        }
      },
      {
        "id": 5,
        "title": "Executor Status",
        "type": "stat",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "executor_status",
            "legendFormat": "Status",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        },
        "valueMappings": [
          {"value": 1, "text": "UP", "color": "green"},
          {"value": 0, "text": "DOWN", "color": "red"}
        ]
      }
    ]
  }
}

